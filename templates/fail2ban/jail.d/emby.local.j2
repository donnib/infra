[emby]
enabled = false
action   = cloudflare-apiv4
           iptables-allports
           telegram
port = 80,443,8096
protocol = tcp
filter = emby
#Number of retrys before to ban
maxretry = 3
#time in seconds
bantime = 3600
findtime = 3600
logpath = /home/{{ main_username }}/docker/emby/config/logs/embyserver.txt
